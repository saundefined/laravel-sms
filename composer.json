{
  "name": "saundefined/laravel-sms",
  "description": "Laravel SMS Providers",
  "keywords": [
    "sms",
    "providers",
    "laravel"
  ],
  "type": "library",
  "homepage": "https://github.com/saundefined/laravel-sms",
  "license": "MIT",
  "authors": [
    {
      "name": "Sergey Panteleev",
      "email": "sergey@php.net"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~6.0",
    "illuminate/http": "~5.7.0|~5.8.0|^6.0|^7.0",
    "illuminate/support": "~5.7.0|~5.8.0|^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4",
    "mockery/mockery": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Saundefined\\LaravelSMS\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Saundefined\\LaravelSMS\\Test\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0-dev"
    },
    "laravel": {
      "providers": [
        "Saundefined\\LaravelSMS\\ServiceProvider"
      ],
      "aliases": {
        "SMS": "Saundefined\\LaravelSMS\\Facades\\LaravelSMS"
      }
    }
  }
}
